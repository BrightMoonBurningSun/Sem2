#include <iostream>
#include <math.h>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {
    double n, summ=0, col, chis, sra, a;
    cin >> a;
    for (n=1; n<=a; n++) {
        cin >> chis;
        summ += chis;
    }
    sra = summ / a;
    cout << fixed << setprecision(10) << sra << endl;
    system("pause");
}

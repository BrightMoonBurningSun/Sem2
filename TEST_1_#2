#include <iostream>
#include <math.h>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {
    double n, summ=0, chis, sred, a;
    cin >> a;
    for (n=1; n<=a; n++) {
        cin >> chis;
        summ += chis;
    }
    sred = summ / a;
    cout << fixed << setprecision(10) << sred << endl;
    system("pause");
}
